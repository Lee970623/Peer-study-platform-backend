<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.elec5619.peerhelping.domain.CoursesEntity" table="courses" schema="peerhelping">
        <id name="courseId">
            <column name="courseId" sql-type="int"/>
        </id>
        <property name="description">
            <column name="description" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="courseCode">
            <column name="courseCode" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>